<h1>Listing tickets</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Dni</th>
    <th>Valor</th>
    <th>Cantidad</th>
    <th>Pagadas</th>
    <th>Empieza</th>
  </tr>

<% @tickets.each do |ticket| %>
  <tr>
    <td><%=h ticket.nombre %></td>
    <td><%=h ticket.dni %></td>
    <td><%=h ticket.valor %></td>
    <td><%=h ticket.cantidad %></td>
    <td><%=h ticket.pagadas %></td>
    <td><%=h ticket.empieza %></td>
    <td><%= link_to 'Show', ticket %></td>
    <td><%= link_to 'Edit', edit_ticket_path(ticket) %></td>
    <td><%= link_to 'Destroy', ticket, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New ticket', new_ticket_path %>